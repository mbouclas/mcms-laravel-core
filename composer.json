{
    "name": "idea-seven/core",
    "type": "library",
    "description": "The core of the CMS",
    "version": "master",
    "dist": {
        "url": "git@bitbucket.org:ideaseven/package-core.git",
        "type": "git"
    },
    "keywords": [
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Bouclas",
            "email": "mbouclas@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "dingo/api": "1.0.x@dev",
        "teepluss/theme": "^2.0",
        "bosnadev/repositories": "0.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "bican/roles": "2.1.*",
        "cocur/slugify": "^2.1",
        "themsaid/laravel-multilingual": "^1.0",
        "arrilot/laravel-widgets": "^3.3",
        "laracasts/utilities": "~2.0",
        "mcamara/laravel-localization": "1.1.*",
        "barryvdh/laravel-translation-manager": "0.2.x-dev",
        "kalnoy/nestedset": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "IdeaSeven\\Core\\": "src"
        },
        "psr-0": {
            "IdeaSeven\\Core": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IdeaSeven\\Core\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
